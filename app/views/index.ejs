<!DOCTYPE html>
<html>
  <head>
    <!-- PWA -->
    <meta charset="UTF-8" />
    <meta name="theme-color" content="#00A85A" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="apple-touch-icon" href="images/icon_256.png" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- Stylesheets -->
    <link rel="stylesheet" href="stylesheets/global.css" />
    <link rel="stylesheet" href="stylesheets/components/menu/menu.css" />
    <link
      rel="stylesheet"
      href="stylesheets/components/container/container.css"
    />
    <link rel="stylesheet" href="stylesheets/components/footer/footer.css" />

    <!-- Application -->
    <link rel="manifest" href="manifest.json" />
    <link rel="shortcut icon" href="images/icon_256.png" type="image/png" />

    <title>Carteirinha Digital - ASSIFEMS</title>
  </head>
  <body>
    <!-- Header -->
    <header>
      <div>
        <img class="assifems" src="/images/assifems.png" />
      </div>

      <div class="options">
        <div id="return">
          <span>Nome de usuário</span
          ><img class="icon_arrow" src="images/icons/arrow.svg" />
        </div>

        <img id="hamburger" src="images/icons/hamburger.svg" onclick="Menu()" />

        <div id="more_options" class="animate__animated animate__backInUp">
          <div class="up">
            <div>Logado em <b>Nome de usuario</b></div>
            <div class="selected">Vizualizar carteirinha</div>
            <div>Estabelecimentos</div>
            <div>Configurações</div>
            <div>Sair</div>
          </div>

          <div class="down">
            <div>
              <img class="icon_help" src="images/icons/help_mini.svg" /> Preciso
              de ajuda
            </div>
          </div>
        </div>
      </div>
    </header>

    <!-- Main Div -->
    <container>
      <!-- Desktop Title -->
      <div class="title">
        CARTEIRINHA DIGITAL<a href="https://wa.me/556781321492"
          ><img src="images/icons/help.svg"
        /></a>
      </div>
      <!-- Title End -->

      <!-- Main Content -->
      <div class="page">
        <!-- Left Side -->
        <div class="left">
          <div class="photo"></div>

          <span>Status:<span class="status">Inválida</span></span>
        </div>

        <!-- Left End -->

        <!-- Right Side -->
        <div class="right">
          <h1>Dados Pessoais</h1>

          <!-- First Line -->
          <div class="lines">
            <div class="data">
              <span class="red">Nome e Sobrenome:</span>
              <span class="name">André Luís de Souza Lopes</span>
            </div>
          </div>

          <!-- First Line End -->

          <!-- Second Line -->
          <div class="lines">
            <!--Birth Date -->
            <div class="data">
              <span class="red">Data de Nascimento</span>
              <span>99/99/9999</span>
            </div>

            <!--Birth Date End -->

            <!--CPF -->
            <div class="data">
              <span class="red">CPF</span>
              <span>999.999.999-99</span>
            </div>

            <!--CPF End -->

            <!--RG -->
            <div class="data">
              <span class="red">RG</span>
              <span>999.999.999-99</span>
            </div>
            <!--RG End -->
            </div>  
            <!-- Second Line End -->

            <!--Third Line -->
            <div class="lines">
              <!--SIAPE -->
              <div class="data">
                <span class="red">SIAPE</span>
                <span>999.999.999-99</span>
              </div>
              <!--SIAPE End -->

              <!--Validity -->
              <div class="data">
                <span class="red">Validade da Carteirinha</span>
                <span>99/99/9999</span>
              </div>
              <!--Validity End -->
            </div>
            <!--Third Line End -->
        </div>
        <!-- Right Side End -->
      </div>
      <!-- Main Content End -->
    </container>

    <!-- Footer -->
    <footer>Desenvolvido por André Lopes, Igor Santana e Juan Silva.</footer>

    <script src="js/script.js"></script>
  </body>
</html>
